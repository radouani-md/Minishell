mradouan $USER mradouan USER mradouan 'mradouan' 'USER' 'mradouan''' 'mradouan'''
mradouan $USER mradouan USER mradouan 'mradouan' $'USER' 'mradouan''' 'mradouan'''

$USER $"USER" "$USER" "$"USER"" "$USER""" mradouan $USER mradouan mradouan
